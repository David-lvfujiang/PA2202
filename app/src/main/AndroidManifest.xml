<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fenda.onn">

    <application
        android:name=".AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.PrivacyPolicyActivity"></activity>
        <activity android:name=".ui.activity.AboutDeviceActivity" />
        <activity android:name=".ui.activity.FmRadioActivity" />
        <activity android:name=".ui.activity.LightDjActivity" />
        <activity android:name=".ui.activity.ProductionSeriesActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.MainActivity" />
        <activity android:name=".ui.activity.ProductionTypeActivity" />
        <activity android:name=".ui.activity.DeviceConnectionActivity" />

        <meta-data
            android:name="design_width_in_dp"
            android:value="400"/>
        <meta-data
            android:name="design_height_in_dp"
            android:value="480"/>
    </application>

</manifest>